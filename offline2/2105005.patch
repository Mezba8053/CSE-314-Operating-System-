+int main(int argc, char *argv[])
+{
+    if (argc == 1)
+    {
+        struct syscall_stat stat;
+        for (int i = 1; i <= maxsyscall; i++)
+            if (history(i, &stat) >= 0 )
+            {
+                printf("%d: syscall: %s, #: %d, time: %d\n",
+                       i, stat.syscall_name, stat.count, stat.accum_time);
+            }
+        exit(0);
+    }
+    int num = atoi(argv[1]);
+    struct syscall_stat stat;
+    if (history(num, &stat) >= 0||stat.count>=0)
+    {
+        printf("%d: syscall: %s, #: %d, time: %d\n", num, stat.syscall_name, stat.count, stat.accum_time);
