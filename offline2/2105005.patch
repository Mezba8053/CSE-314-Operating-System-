+int dummy_arr[200];
+
+int main(int argc, char *argv[])
+{
+  int tickets = -1;
+  if (argc > 1)
+  {
+    tickets = atoi(argv[1]);
+  }
+
+  if (settickets(tickets) < 0)
+  {
+    printf("Failed to set tickets = %d\n", tickets);
+    exit(1);
+  }
+
+  printf("PID %d: Set tickets to %d\n", getpid(), tickets);
+
+  fork();
+  fork();
+  fork();
+
+  for (int i = 1; i <= 1000000; i++)
